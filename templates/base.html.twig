<!-- /form register -->
<script>
    $(document).ready(function() {
        $("#myModal2 form").submit(function() {
            event.preventDefault();
            let isVide = false
            for (let i = 0; i < $("#myModal2 form input").length; i++) {
                if ($("#myModal2 form input").eq(i).val().trim().length < 1)
                    isVide = true
            }
            if (!isVide) {
                let data = $(this).serialize();

                $.post(
                    "/register",
                    data,
                    function(data) {
                        $("#myModal2 form").css("display", "none");
                        $("#msg").html("<p class='text-center alert alert-success col-md-8 offset-md-2'>Bienvenue sur notre boutique !</p>");
                        setTimeout(function() {
                            document.location.href = "/"; // redirection
                        }, 3000);
                    })
            } else {
                alert("Champs vide !")
            }


        })

    })

$('#confirm_password').on('keyup', function () {
    if ($(this).val() == $('#password').val()) {
        $('#message').html('Les mots de passe sont identiques').css('color', 'green');
    } else $('#message').html("Les mots de passe ne sont pas identiques").css('color', 'red');
});
function check_pass() {
    if (document.getElementById('password').value ==
            document.getElementById('confirm_password').value) {
        document.getElementById('sub').disabled = false;
    } else {
        document.getElementById('sub').disabled = true;
    }
}
</script>

